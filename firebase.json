{
  "hosting": {
    "source": "client",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },    "rewrites": [
      {
        "source": "/api/accounts/**",
        "destination": "/api/accounts"
      },
      {
        "source": "/api/**",
        "destination": "/api"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
